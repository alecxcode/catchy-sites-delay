{
  "manifest_version": 2,
  "name": "CatchySitesDelay",
  "version": "0.3",
  "description": "Makes a delay for some websites to not waste your time",
  "icons": {
    "48": "icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["catchy.js"],
      "run_at": "document_start"
    }
  ],
  "applications": {
    "gecko": {
      "id": "catchysitesdelay@mozilla.org"
    }
  }
}
